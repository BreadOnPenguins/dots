# /etc/gimp/3.0/gimprc
# Most values can be set within GIMP by changing some options in the
# Preferences dialog.

(prev-language "American English")
(config-version "3.0.6")
(default-image
    (icon-name "gimp-display")
    (width 3840)
    (height 2400)
    (unit pixels)
    (xresolution 300)
    (yresolution 300)
    (resolution-unit inches)
    (image-type rgb)
    (precision u16-non-linear)
    (color-profile NULL)
    (simulation-profile NULL)
    (simulation-bpc no)
    (simulation-intent relative-colorimetric)
    (fill-type transparent)
    (comment ""))
(thumbnail-size none)
(save-document-history no)
(import-raw-plug-in "")
(image-title-format "%f-%p.%i -- %wx%h -- (%t)")
(monitor-xresolution 94)
(monitor-yresolution 94)
(default-view
